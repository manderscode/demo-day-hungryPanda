    </div>

    <div class="row">

       
        <div class="col-sm-6">
          <h3>Posts</h3>
          <ul class="messages">
          <% for(var i=0; i<posts.length; i++) {%>
            <li class="message">
              <a href="/post/<%= posts[i]._id %>">
                <h4><%= posts[i].caption %></h4>
                <img src="<%= posts[i].img %>" alt="">
                <a href="/page/<%= posts[i].postedBy %>"><%= posts[i].postedBy %></a>
              </a>  
            </li>
          <% } %>
          </ul>


</div>

